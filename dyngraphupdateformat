' Graficas de Accesibilidad
    Sheets("Acc Analysis").Select
    ActiveSheet.ChartObjects("graf_Acc_S1").Activate
    ActiveChart.ApplyChartTemplate ( _
        "C:\Users\Fredy Avellan\AppData\Roaming\Microsoft\Templates\Charts\Acc_NIC.crtx" _
        )
    ActiveChart.ChartTitle.Select
    ActiveChart.ChartTitle.Text = "Accesibility % / Sector - S1"
    Selection.Format.TextFrame2.TextRange.Characters.Text = _
        "Accesibility % / Sector - S1"
    With Selection.Format.TextFrame2.TextRange.Characters(1, 28).ParagraphFormat
        .TextDirection = msoTextDirectionLeftToRight
        .Alignment = msoAlignCenter
    End With
    With Selection.Format.TextFrame2.TextRange.Characters(1, 28).Font
        .BaselineOffset = 0
        .Bold = msoTrue
        .NameComplexScript = "+mn-cs"
        .NameFarEast = "+mn-ea"
        .Fill.Visible = msoTrue
        .Fill.ForeColor.RGB = RGB(0, 0, 0)
        .Fill.Transparency = 0
        .Fill.Solid
        .Size = 20
        .Italic = msoFalse
        .Kerning = 12
        .Name = "+mn-lt"
        .UnderlineStyle = msoNoUnderline
        .Spacing = 0
        .Strike = msoNoStrike
    End With
'    ActiveChart.ChartArea.Select

        
